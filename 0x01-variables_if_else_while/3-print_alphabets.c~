#include <stdio.h>
/**
* main - a function to print the alphabet
*in lowercase, and then in uppercase.
*Return: 0 under normal circumstances 
 */
int main(void)
{
char letter;
for (letter = 'a'; letter <= 'z'; letter++)
putchar(letter);
for (letter = 'A'; letter <= 'Z'; letter++)
putchar(letter);
putchar('\n');
return (0);
}
